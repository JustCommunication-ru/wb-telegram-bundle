{
    "name": "justcommunication-ru/telegram-bundle",
    "description": "Telegram bundle",
    "type": "symfony-bundle",
    "keywords": ["telegram","messages","notification"],
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "justcommunication-ru/cache-bundle": "^0.1",
        "justcommunication-ru/func-bundle": "^0.1",

        "symfony/http-kernel": ">=5.4 ^6.2",
        "symfony/config": ">=5.4 ^6.2",
        "symfony/dependency-injection": ">=5.4 ^6.2",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/orm": "^2.15",
        "symfony/framework-bundle": ">=5.4 ^6.2",
        "symfony/yaml": ">=5.4 ^6.2"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:JustCommunication-ru/cache-bundle.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:JustCommunication-ru/func-bundle.git"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "JustCommunication\\TelegramBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JustCommunication\\TelegramBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/browser-kit": "^6.3",
        "symfony/phpunit-bridge": "^6.3"
    }
}
